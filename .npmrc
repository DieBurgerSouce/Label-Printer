# Screenshot_Algo npm Configuration
# Enterprise-grade npm settings for consistency across teams

# Version Management
save-exact=true
engine-strict=true

# Security
audit-level=moderate

# Performance & Behavior
legacy-peer-deps=false
fund=false
progress=true

# Registry (default npm, can be overridden for private packages)
# registry=https://registry.npmjs.org/

# Package Lock
package-lock=true

# Logging
loglevel=warn
