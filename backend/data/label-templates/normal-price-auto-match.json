{
  "id": "normal-price-auto-match",
  "name": "Standard-Label (Normalpreis)",
  "width": 800,
  "height": 600,
  "widthMm": 100,
  "heightMm": 75,
  "autoMatchEnabled": true,
  "rules": {
    "id": "rule-normal-price",
    "enabled": true,
    "conditions": [
      {
        "field": "priceType",
        "operator": "is",
        "value": "normal"
      }
    ],
    "logic": "AND"
  },
  "elements": [
    {
      "id": "title",
      "type": "text",
      "x": 20,
      "y": 20,
      "width": 360,
      "height": 30,
      "content": "{{productName}}",
      "fontSize": 18,
      "fontWeight": "bold",
      "color": "#000000",
      "align": "center"
    },
    {
      "id": "article-num",
      "type": "articleNumber",
      "x": 20,
      "y": 60,
      "width": 150,
      "height": 20,
      "content": "Art.Nr.: {{articleNumber}}",
      "fontSize": 12,
      "fontWeight": "normal",
      "color": "#666666",
      "align": "left"
    },
    {
      "id": "product-image",
      "type": "image",
      "x": 20,
      "y": 90,
      "width": 120,
      "height": 120,
      "content": "{{productImage}}"
    },
    {
      "id": "single-price",
      "type": "price",
      "x": 160,
      "y": 120,
      "width": 220,
      "height": 60,
      "content": "{{price}} {{currency}}",
      "fontSize": 36,
      "fontWeight": "bold",
      "color": "#FF0000",
      "align": "center"
    },
    {
      "id": "qr-code",
      "type": "qrCode",
      "x": 300,
      "y": 60,
      "width": 80,
      "height": 80,
      "content": "{{sourceUrl}}"
    },
    {
      "id": "description-short",
      "type": "description",
      "x": 20,
      "y": 220,
      "width": 360,
      "height": 20,
      "content": "{{description}}",
      "fontSize": 10,
      "fontWeight": "normal",
      "color": "#666666",
      "align": "left",
      "maxLines": 1
    }
  ],
  "settings": {
    "backgroundColor": "#ffffff",
    "backgroundImageOpacity": 0.5,
    "defaultFontFamily": "Arial",
    "defaultFontColor": "#000000",
    "defaultFontSize": 14,
    "borderEnabled": true,
    "borderColor": "#d1d5db",
    "borderWidth": 1,
    "borderRadius": 4,
    "shadowEnabled": false,
    "shadowColor": "#000000",
    "shadowBlur": 5,
    "padding": 10
  }
}