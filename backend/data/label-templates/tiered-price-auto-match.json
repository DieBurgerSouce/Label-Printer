{
  "id": "tiered-price-auto-match",
  "name": "Staffelpreis-Label (Mengenrabatt)",
  "width": 1600,
  "height": 1200,
  "widthMm": 100,
  "heightMm": 75,
  "autoMatchEnabled": true,
  "rules": {
    "id": "rule-tiered-price",
    "enabled": true,
    "conditions": [
      {
        "field": "priceType",
        "operator": "is",
        "value": "tiered"
      }
    ],
    "logic": "AND"
  },
  "elements": [
    {
      "id": "title",
      "type": "text",
      "x": 40,
      "y": 40,
      "width": 720,
      "height": 50,
      "content": "{{productName}}",
      "fontSize": 32,
      "fontWeight": "bold",
      "color": "#000000",
      "align": "center"
    },
    {
      "id": "article-num",
      "type": "articleNumber",
      "x": 40,
      "y": 110,
      "width": 300,
      "height": 40,
      "content": "Art.Nr.: {{articleNumber}}",
      "fontSize": 22,
      "fontWeight": "normal",
      "color": "#666666",
      "align": "left"
    },
    {
      "id": "product-image",
      "type": "image",
      "x": 40,
      "y": 170,
      "width": 200,
      "height": 200,
      "content": "{{productImage}}"
    },
    {
      "id": "tiered-prices-display",
      "type": "tieredPrices",
      "x": 280,
      "y": 230,
      "width": 480,
      "height": 240,
      "content": "{{tieredPricesText}}",
      "fontSize": 24,
      "fontWeight": "normal",
      "color": "#000000",
      "align": "left",
      "backgroundColor": "#FFF8DC",
      "borderEnabled": true,
      "borderColor": "#FFD700",
      "borderWidth": 4,
      "padding": 16
    },
    {
      "id": "qr-code",
      "type": "qrCode",
      "x": 600,
      "y": 110,
      "width": 160,
      "height": 160,
      "content": "{{sourceUrl}}"
    },
    {
      "id": "description",
      "type": "description",
      "x": 40,
      "y": 500,
      "width": 1000,
      "height": 280,
      "content": "{{description}}",
      "fontSize": 18,
      "fontWeight": "normal",
      "color": "#666666",
      "align": "left",
      "maxLines": 8
    }
  ],
  "settings": {
    "backgroundColor": "#ffffff",
    "backgroundImageOpacity": 0.5,
    "defaultFontFamily": "Arial",
    "defaultFontColor": "#000000",
    "defaultFontSize": 14,
    "borderEnabled": true,
    "borderColor": "#FF6B6B",
    "borderWidth": 2,
    "borderRadius": 6,
    "shadowEnabled": true,
    "shadowColor": "#000000",
    "shadowBlur": 3,
    "padding": 10
  }
}