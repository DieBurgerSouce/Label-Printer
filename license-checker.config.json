{
  "$schema": "https://json.schemastore.org/license-checker-config.json",
  "production": true,
  "development": false,
  "start": ".",
  "onlyAllow": [
    "MIT",
    "Apache-2.0",
    "BSD-2-Clause",
    "BSD-3-Clause",
    "ISC",
    "0BSD",
    "CC0-1.0",
    "CC-BY-3.0",
    "CC-BY-4.0",
    "Unlicense",
    "WTFPL",
    "Python-2.0",
    "BlueOak-1.0.0",
    "Artistic-2.0",
    "Zlib",
    "MPL-2.0"
  ],
  "failOn": [
    "GPL",
    "GPL-2.0",
    "GPL-2.0-only",
    "GPL-2.0-or-later",
    "GPL-3.0",
    "GPL-3.0-only",
    "GPL-3.0-or-later",
    "AGPL",
    "AGPL-3.0",
    "AGPL-3.0-only",
    "AGPL-3.0-or-later",
    "LGPL",
    "LGPL-2.0",
    "LGPL-2.1",
    "LGPL-3.0",
    "SSPL-1.0",
    "BUSL-1.1",
    "Commons-Clause"
  ],
  "excludePackages": [],
  "excludePrivatePackages": true,
  "direct": 0,
  "summary": true,
  "json": false,
  "csv": false,
  "out": null,
  "customPath": null,
  "unknown": false,
  "clarificationsFile": "./license-clarifications.json",
  "packages": [],
  "excludePackagesStartingWith": [
    "@types/"
  ]
}
